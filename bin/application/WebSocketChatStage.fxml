<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="690.0" prefWidth="782.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.WebSocketChatStageController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Nazwa użytkownika:" />
      <TextField fx:id="userTextField" Text="user" layoutX="128.0" layoutY="10.0" prefHeight="25.0" prefWidth="581.0" promptText="wpisz swoją nazwę użytkownika" />
      <Button fx:id="btnSet" layoutX="719.0" layoutY="10.0" mnemonicParsing="false" onAction="#btnSet_Click" text="Ustaw" />
      <TextArea fx:id="chatTextArea" editable="false" layoutX="128.0" layoutY="44.0" prefHeight="426.0" prefWidth="581.0" />
      <Label layoutX="25.0" layoutY="479.0" text="Nowa wiadomość:" />
      <TextField fx:id="messageTextField" layoutX="128.0" layoutY="475.0" onKeyPressed="#messageTextField_KeyPressed" prefHeight="25.0" prefWidth="581.0" />
      <Button fx:id="btnSend" layoutX="719.0" layoutY="475.0" mnemonicParsing="false" onAction="#btnSend_Click" prefHeight="25.0" prefWidth="49.0" text="Wyślij" />
      <Label layoutX="68.0" layoutY="563.0" text="Załączniki:" />
      <ListView fx:id="attachmentsListView" layoutX="128.0" layoutY="509.0" onMouseClicked="#attachmentsListView_doubleClick" prefHeight="141.0" prefWidth="581.0" />
      <Button fx:id="btnChooseAndSendAttachment" layoutX="561.0" layoutY="650.0" mnemonicParsing="false" onAction="#btnChooseAndSendAttachment_Click" text="Wybierz i wyślij załącznik" />
      <Label layoutX="53.0" layoutY="249.0" text="Wiadomości:" />
      <Label layoutX="128.0" layoutY="654.0" text="Aby zapisać załącznik z listy, klinkij na niego dwukrotnie.">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
